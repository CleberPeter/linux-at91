   label = "at91bootstrap";
   reg = <0x0 0x40000>;
  };

  bootloader@40000 {
   label = "bootloader";
   reg = <0x40000 0x80000>;
  };

  bootloaderenv@c0000 {
   label = "bootloader env";
   reg = <0xc0000 0xc0000>;
  };

  dtb@180000 {
   label = "device tree";
   reg = <0x180000 0x80000>;
  };

  kernel@200000 {
   label = "kernel";
   reg = <0x200000 0x600000>;
  };

  rootfs@800000 {
   label = "rootfs";
   reg = <0x800000 0x0f800000>;
  };
 };
};

&dbgu {
 status = "okay";

 atmel,use-dma-rx;
};

&usb0 {
 status = "okay";

 atmel,vbus-gpio = <&pioD 28 0>;
 pinctrl-names = "default";
 pinctrl-0 = <&pinctrl_usba_vbus>;
};
