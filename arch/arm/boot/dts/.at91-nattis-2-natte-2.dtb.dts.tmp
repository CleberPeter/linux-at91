   #address-cells = <1>;
   #size-cells = <0>;

   charger@9 {
    compatible = "ti,bq24735";
    reg = <0x9>;

    ti,charge-current = <2000>;
    ti,charge-voltage = <16800>;

    poll-interval = <20000>;
   };
  };
 };
};

&i2c0 {
 status = "okay";

 ioexp: ioexp@20 {
